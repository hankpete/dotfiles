! Xresources file.
! xrdb -merge ~/.Xresources to sync

!!!!!!URxvt stuff

URxvt.font: xft:monospace:size=10

URxvt.scrollBar: false

URxvt.letterspace: -5

! perl extensions
URxvt.perl-lin: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,tabbed,matcher,selection-to-clipboard

! tab colors
URxvt.tabbed.tabbar-fg: 3
URxvt.tabbed.tabbar-bg: 0
URxvt.tabbed.tab-fg: 3
URxvt.tabbed.tab-bg: 1

! clickable urls
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1 

